<Window x:Class="FruitWarehouse.Views.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:FruitWarehouse.ViewModels"
        Title="Игровое поле" Height="400" Width="600">
    <Window.DataContext>
        <local:GameViewModel />
    </Window.DataContext>
    <Grid>
        <Button Content="Меню" Command="{Binding BackToMenuCommand}" CommandParameter="{Binding RelativeSource={RelativeSource AncestorType=Window}}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10" Width="100"/>
        
    </Grid>
</Window>